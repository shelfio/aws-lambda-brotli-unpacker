{
  "name": "@shelf/aws-lambda-brotli-unpacker",
  "version": "2.0.0",
  "description": "Unpacks large Lambda binaries to /tmp",
  "keywords": [
    "lambda",
    "brotli"
  ],
  "repository": "shelfio/aws-lambda-brotli-unpacker",
  "license": "MIT",
  "author": {
    "name": "Vlad Holubiev",
    "email": "vlad@shelf.io",
    "url": "shelf.io"
  },
  "main": "lib",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "rm -rf lib/ && yarn build:types && yarn build:code",
    "build:code": "babel src --out-dir lib --ignore '**/*.test.ts' --extensions '.ts' && find ./lib -name '*.test.d.ts' -delete",
    "build:types": "tsc --emitDeclarationOnly --declaration --isolatedModules false --declarationDir lib",
    "coverage": "yarn test --coverage",
    "lint": "yarn lint:ci --fix",
    "lint:ci": "eslint . --ext .js,.ts,.json",
    "prepack": "yarn build",
    "test": "bin/test",
    "test:internal": "rm -rf .test && mkdir .test && jest",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch"
  },
  "lint-staged": {
    "*.{html,md,yml}": [
      "prettier --write"
    ],
    "*.{js,ts,json}": [
      "eslint --fix"
    ]
  },
  "babel": {
    "extends": "@shelf/babel-config/backend"
  },
  "prettier": "@shelf/prettier-config",
  "dependencies": {
    "tar-fs": "2.1.1"
  },
  "devDependencies": {
    "@babel/cli": "7.17.6",
    "@babel/core": "7.17.5",
    "@shelf/babel-config": "0.1.8",
    "@shelf/eslint-config": "2.13.0",
    "@shelf/prettier-config": "1.0.0",
    "@shelf/tsconfig": "0.0.6",
    "@types/jest": "27.4.1",
    "@types/node": "14",
    "@types/tar-fs": "2.0.1",
    "eslint": "8.14.0",
    "husky": "7.0.4",
    "jest": "27.5.1",
    "lint-staged": "12.4.1",
    "prettier": "2.5.1",
    "typescript": "4.6.3"
  },
  "engines": {
    "node": ">=14"
  },
  "publishConfig": {
    "access": "public"
  }
}
