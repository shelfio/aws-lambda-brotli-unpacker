{
  "name": "@shelf/aws-lambda-brotli-unpacker",
  "version": "2.1.0",
  "description": "Unpacks large Lambda binaries to /tmp",
  "keywords": [
    "lambda",
    "brotli"
  ],
  "repository": "shelfio/aws-lambda-brotli-unpacker",
  "license": "MIT",
  "author": {
    "name": "Vlad Holubiev",
    "email": "vlad@shelf.io",
    "url": "shelf.io"
  },
  "sideEffects": false,
  "type": "module",
  "exports": "./lib/index.js",
  "module": "./lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "rm -rf lib/ && tsc",
    "coverage": "yarn test --coverage",
    "lint": "yarn lint:ci --fix",
    "lint:ci": "eslint . --quiet",
    "prepack": "yarn build",
    "test": "bin/test",
    "test:internal": "rm -rf .test && mkdir .test && jest",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch"
  },
  "lint-staged": {
    "*.{html,md,yml}": [
      "prettier --write"
    ],
    "*.{js,ts,json}": [
      "eslint --fix"
    ]
  },
  "prettier": "@shelf/prettier-config",
  "dependencies": {
    "tar-fs": "3.0.8"
  },
  "devDependencies": {
    "@shelf/eslint-config": "5.0.1",
    "@shelf/prettier-config": "1.0.0",
    "@shelf/tsconfig": "0.1.0",
    "@swc/core": "1.11.18",
    "@swc/jest": "0.2.37",
    "@types/jest": "29.5.14",
    "@types/node": "20",
    "@types/tar-fs": "2.0.4",
    "eslint": "9.24.0",
    "husky": "9.1.7",
    "jest": "29.7.0",
    "jest-junit": "16.0.0",
    "lint-staged": "15.5.1",
    "prettier": "3.5.3",
    "ts-jest-resolver": "2.0.1",
    "typescript": "5.8.3"
  },
  "engines": {
    "node": ">=20"
